{
  "name": "EAA Tester",
  "version": "0.0.1",
  "description": "EAA Tester",
  "license": "Apache",
  "author": {
    "name": "Rui Shen",
    "email": "shenrui@cn.ibm.com"
  },
  "main": "bin/eaatester",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/silverhook-dslink-java"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/silverhook-dslink-java/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "eaatester"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "eaa.tester.dslink.Main"
    }
  }
}
